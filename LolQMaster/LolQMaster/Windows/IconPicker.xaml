<Window x:Class="LolQMaster.Windows.IconPicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LolQMaster.Windows"
        mc:Ignorable="d"
        Title="LolQMaster - Pick Icon" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
            <StackPanel Name="FilterRow" Orientation="Horizontal" Height="30" Grid.Row="0">
                
            </StackPanel>
            <ScrollViewer Height="auto" Grid.Row="1">
                <WrapPanel Name="ContentPanel">

                </WrapPanel>
            </ScrollViewer>
    </Grid>
</Window>
