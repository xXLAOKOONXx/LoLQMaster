<Window x:Class="LolQMaster.Windows.AddQueue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LolQMaster.Windows"
        mc:Ignorable="d"
        Title="LolQMaster - Add Queue" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <ScrollViewer Grid.Column="0">
            <StackPanel Name="QueuePicker">

            </StackPanel>
        </ScrollViewer>
        <Image Name="IMGSummonerIcon" Grid.Column="1" Width="150" Height="150" Margin="123,36,123,234"/>
        <Button Name="BTNPickIcon" Grid.Column="1" Content="Pick Icon" Margin="123.2,191,123.6,194" Click="Button_Click"/>
        <Button Name="BTNAddQueue" Grid.Column="1" Content="Add Queue and Icon" Margin="227.2,351,35.6,34" Click="BTNAddQueue_Click"/>
    </Grid>
</Window>
